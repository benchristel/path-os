{
  "description": "Path OS",
  "manifest_version": 2,
  "name": "Path OS",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scrollbars.js"],
      "all_frames": true
    }
  ]
}
